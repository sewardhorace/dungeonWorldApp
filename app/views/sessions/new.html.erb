<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1>Log In</h1>
      <div class="row">
        <div class="col-xs-3">
          <%= form_for(:session, url: login_path) do |f| %>
            <%= f.email_field :email, :placeholder => "Email" %>
            <%= f.password_field :password, :placeholder => "Password" %>
            <%= f.submit "Log in", class: "btn btn-default" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
